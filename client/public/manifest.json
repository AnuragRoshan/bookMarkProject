{
  "name": "BookmarkManager",
  "version": "0.1",
  "description": "A simple bookmark manager",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "http://localhost:3000/*",
    "storage",
    "activeTab",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    }
  }
}
